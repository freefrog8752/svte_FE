/*page start  */

.dib {
  display: inline-block;
}

page {
  border-top: 1px solid #e0e0e0;
  font-size: 14px;
  line-height: 1.4;
  color: #37474f;
  background-color: #eceff1;
}

.gw-cardlist-h1 {
  margin: 30rpx;
}

.cardlist-lesson-list-title {
  font-size: 28rpx;
  font-weight: bold;
  color: #354750;
}

.gw-cardlist-h2 {
  margin: 8rpx 0 0;
  color: #B0BEC5;
  font-size: 20rpx;
}

.gw-cardlist-item {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  padding: 40rpx 60rpx 40rpx 30rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow:hidden;
}

.gw-cardlist-item-left {
  position: relative;
  margin-right: 40rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  align-self: center;
}

.gw-cardlist-item-left.blue {
  background-color: #1976d2;
}

.gw-cardlist-item-left.green {
  background-color: #4db6ac;
}

.gw-cardlist-item-img {
  position: absolute;
  top: -140rpx;
  left: -140rpx;
  width: 400rpx;
  height: 400rpx;
}

.gw-cardlist-item-cont {
  flex: 1;
}

.gw-cardlist-item-top, .gw-cardlist-item-bottom {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}

.gw-cardlist-item-bottom {
  margin-top: 10rpx;
  align-items: center;
}

.gw-cardlist-item-top-left {
  color: #354750;
  font-size: 28rpx;
  line-height: 36rpx;
  font-weight: bold;
}

.gw-cardlist-item-top-right {
  color: #37474f;
}

.gw-btn {
  margin: 0;
  padding: 0 40rpx;
  /**display: inline-block;**/
  min-width: 96px;
  line-height: 40px;
  border-radius: 30px;
  font-size: 13px;
  color: #fff;
  font-weight: bold;
  background-color: #006469;
  white-space: nowrap;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  height: 40px;
}

.swap-btn {
  color: #596d79;
  margin: 0;
  padding: 0 40rpx;
  display: inline-block;
  min-width: 96px;
  line-height: 40px;
  font-size: 13px;
  font-weight: bold;
  white-space: nowrap;
  background-color: rgba(255, 255, 255, 0);
}

.gw-cardlist-item-bottom-left {
  padding-right: 4px;
  color: #546e7a;
  font-size: 20rpx;
}

.gw-cardlist-item-bottom-left [class^='gwicon-'] {
  display: inline-block;
  margin-right: 1px;
  font-size: 24rpx;
  vertical-align: middle;
}

.gw-cardlist-item-right {
  display: flex;
  width: 40rpx;
  flex-direction: column;
  justify-content: center;
}

.gw-cardlist-item-right .tip-begin-learn {
  width: 26rpx;
  height: 40rpx;
}

.gw-cardlist-item-right .tip-complete-learn {
  width: 38rpx;
  height: 26rpx;
}

.gw-btn.blue {
  background-color: #1976d2;
}

.gw-btn.dark {
  background-color: #546e7a;
}

.gw-btn.white {
  background-color: #fff;
  color: #37474f;
}

.gw-cardlist-item.dark {
  background-color: #fafafa;
}

.gw-showmore, .gw-showall {
  line-height: 60px;
  color: #fff;
  text-align: center;
}

.gw-showall {
  font-weight: bold;
}

.gw-menupage-tt {
  margin-top: 8px;
  color: #fff;
}

.gw-menupage-item {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  color: #e0e0e0;
}

.gw-menupage-tt, .gw-menupage-item {
  padding: 0 15px;
  line-height: 52px;
  border-bottom: 1px solid rgba(255, 255, 255, 0.5);
}

.gw-menupage-item-tt {
  font-size: 14px;
}

.gw-menupage-item-nb {
}

.gw-profile-head {
  padding: 10px 14px 8px;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  border-bottom: 1px solid #e0e0e0;
}

.gw-profile-head-m-img {
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  overflow: hidden;
  background-color: #fff;
}

.gw-profile-head-m-img image {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  transform: scale(0.9);
}

.gw-profile-head-l, .gw-profile-head-r {
  width: 40px;
  height: 60px;
  font-size: 20px;
}

.gw-profile-head-r {
  text-align: right;
}

.gw-profile-head-m-tt {
  margin-top: 7px;
  font-size: 14px;
  text-align: center;
}

.gw-profile-h1 {
  margin: 30px 20px 8px;
}

.gw-profile-item {
  margin: 8px;
  padding: 24px 20px 30px;
  background-color: #fff;
}

.gw-profile-item-stage {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
}

.gw-profile-item-stage-left, .gw-profile-item-stage-right {
  text-align: center;
}

.gw-profile-item-stage-nb {
  font-size: 30px;
}

.gw-profile-item-stage-tt {
  margin-top: 4px;
}

.gw-profile-item-info {
  margin-top: 12px;
  text-align: center;
}

.gw-profile-item-info-tt {
  margin-top: 14px;
  font-weight: bold;
}

.gw-profile-item-info-con {
  margin-top: 10px;
}

.gw-profile-item-ctrl {
  margin-top: 26px;
  text-align: center;
}

.gw-profile-item-info .gw-btn {
  margin-top: 25px;
}

.gw-setpage-h1 {
  padding: 0 15px;
  margin-top: 10px;
  line-height: 34px;
  color: #888;
  border-bottom: 1px #dfe2e8 solid;
}

.gw-overlay-wrap {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
