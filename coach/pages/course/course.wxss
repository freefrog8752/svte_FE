.week-header,.week-header-sec{
  height:80rpx;
line-height:80rpx;
display:flex;
justify-content:space-between;
padding:0 20rpx;
color:#fff;
}
.savesetting{background-color: #ffd92e;color:#fff; width: 200rpx;font-size:12px;}
.category{
  padding:40rpx 0;
  display: flex;
  flex-direction: row;
  flex: 1;
  justify-content: space-around;
  align-items: center;
  color:#fff;
}
.yellow{
  color:#ffd92e;
}
.subcate{
  height: 60rpx;
}
.week-header-sec{
  padding: 0;
}
.week-header-cell{
  flex: 1;
  text-align: center;
}
.weekday-table{
  height: 90vh;
  background: #fff;
}
.tr{
  height: 100rpx;
  display: flex;
}
.td{
  border:1rpx solid #c1c1c1;
justify-content:center;
width:100%;
border-collapse:collapse;
flex:1;
align-items:center;
display:flex;
}
.rest{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color:#c1c1c1; 
  text-align: center;
}
.course{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #FFF8DC;
  text-align: center;
}